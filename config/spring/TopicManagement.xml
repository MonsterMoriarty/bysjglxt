<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="topicManagementAction" class="com.bysjglxt.action.TopicManagementAction"
		scope="prototype">
		<property name="topicManagementService" ref="topicManagementService"></property>
	</bean>

	<bean id="topicManagementService" class="com.bysjglxt.service.impl.TopicManagementServiceImpl">
		<property name="topicManagementDao" ref="topicManagementDao"></property>
	</bean>

	<bean id="topicManagementDao" class="com.bysjglxt.dao.impl.TopicManagementDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>


</beans>
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="SectionInformationManagement" namespace="/section"
		extends="struts-default">
		<action name="SectionInformationManagement_*"
			class="com.bysjglxt.action.SectionInformationManagementAction"
			method="{1}">

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="TopicManagement" namespace="/topic"
		extends="struts-default">
		<action name="TopicManagement_*"
			class="com.bysjglxt.action.TopicManagementAction"
			method="{1}">
			<result name="TopicListPage">/WEB-INF/view/TopicManagement/StudentInformationManagement.jsp
			</result>
			<result name="CreateTopicPage">/WEB-INF/view/TopicManagement/CreateTopic.jsp
			</result>
		</action>
	</package>
</struts>
